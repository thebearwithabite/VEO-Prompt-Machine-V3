
{
  "manifest_version": 3,
  "name": "Aether Vault Key",
  "version": "1.0",
  "description": "Auto-injects your GCP Master Token into Aether Studio.",
  "permissions": ["storage", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
